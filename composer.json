{
    "name": ":vendor/:package_name",
    "description": ":package_description",
    "keywords": [
        "laravel",
        "nova"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "homepage": "https://github.com/:vendor/:package_name",
    "license": "MIT",
    "authors": [
        {
            "name": ":author_name",
            "email": ":author_email",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-readline": "*",
        "nova-kit/nova-packages-tool": "^1.0"
    },
    "autoload": {
        "psr-4": {
            ":namespace_vendor\\:namespace_tool_name\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            ":namespace_vendor\\:namespace_tool_name\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                ":namespace_vendor\\:namespace_tool_name\\:namespace_tool_nameToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "tightenco/duster": "^2.7"
    }
}
